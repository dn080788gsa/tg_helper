<context>

    <input pattern="$Text">
        <var name="Utterance" value="$Text" scope="input"/>
        <get url="https://jobs.dou.ua/vacancies/feeds/" var="Result">
            <param name="category" value="Front%20End"/>               
        </get>  
        <post url="https://script.google.com/macros/s/AKfycbywKqLRON-qznZ-gFa2X6ODArJM3M1WO3xqclAaPvfWgwApobM/exec" var="Response">
          <header name="Content-Type" value="application/json"/>
          <body>
            <![CDATA[
              {
                "somevariable" : "$Utterance",
                "res": "$Result"
              }
            ]]>
          </body>
        </post>
        <var name="Answer" value='Привет с GIT'/>
        <output value="$Answer"/>
        <sample>
            <item value="Раз"/>
            <item value="Два"/>
            <item value="Три"/>
        </sample>      
    </input>

</context>
